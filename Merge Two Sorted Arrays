# Enter your code here. Read input from STDIN. Print output to STDOUT
n = int(input())
arr = list(map(int, input().split()))
m = int(input())
arr1 = list(map(int, input().split()))
i = 0
j = 0
while i < n and j < m:
    if arr[i] <= arr1[j]:
        print(arr[i], end=" ")
        i += 1
    else:
        print(arr1[j], end=" ")
        j += 1
while i < n:
    print(arr[i], end=" ")
    i += 1
while j < m:
    print(arr1[j], end=" ")
    j += 1
